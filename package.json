{
  "name": "prime-time",
  "version": "0.1.0",
  "description": "A vanilla JavaScript date library",
  "main": "dist/prime-time.js",
  "types": "dist/prime-time.d.ts",
  "scripts": {
    "clean": "rm -rf dist/*",
    "build": "tsc",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "test-coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "clean-build": "npm run clean && npm run build",
    "clean-build-test": "npm run clean && npm run build && npm run test",
    "clean-build-test-coverage": "npm run clean && npm run build && npm run test-coverage",
    "prerelease": "npm run clean-build && npm run test-coveralls"
  },
  "keywords": [
    "date",
    "npm"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelverschoof/prime-time.git"
  },
  "author": "Michael Verschoof <info@michaelv.nl>",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/michaelverschoof/prime-time/issues"
  },
  "homepage": "https://github.com/michaelverschoof/prime-time#readme",
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "coveralls": "^3.0.9",
    "jest": "^25.1.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.7.5"
  }
}
